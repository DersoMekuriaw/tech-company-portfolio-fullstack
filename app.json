{
  "expo": {
    "name": "TechInnovate Portfolio",
    "slug": "techinnovate-portfolio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4F46E5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.techinnovate.portfolio"
    },
    // "android": {
    //   "adaptiveIcon": {
    //     // "foregroundImage": "assets/adaptive-icon.png",
    //     // "backgroundColor": "#4F46E5"
    //   },
    //   "package": "com.techinnovate.portfolio"
    // },
    "web": {
      "favicon": "assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
